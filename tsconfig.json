{
  "compilerOptions": {
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowJs": true,
    "lib": ["es2021"],
    "outDir": "./dist/"
  },
  "files": [
    "src/index.ts",
    "src/client/message.ts",
    "src/client/messageReactionAdd.ts",
    "src/client/ready.ts",
    "src/client/voiceStateUpdate.ts",
    "src/config/etc.ts",
    "src/config/prototype.ts",
    "src/config/type.ts",
    "src/message/cal.ts",
    "src/message/speak.ts"
  ]
}
